<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.blikoon.qrcodescannerlibrary.Main4Activity"
    android:background="@color/qr_code_white"
    >
    <Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:title="Info."
        android:background="@drawable/fondonombre"
        android:id="@+id/tool"
        ></Toolbar>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Alumnos"
        android:textSize="40dp"
        android:textColor="@color/Dorado"
        android:layout_below="@+id/tool"
        android:id="@+id/titulo"
        />
<TextView
    android:layout_marginTop="10dp"
    android:layout_below="@+id/titulo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/hh"
    android:textColor="@color/qr_code_finder_mask"
    />
</RelativeLayout>
